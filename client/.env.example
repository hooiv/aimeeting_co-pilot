# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_WS_URL=ws://localhost:5000

# Google Integrations
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id
REACT_APP_GOOGLE_CLIENT_SECRET=your_google_client_secret

# Microsoft Integrations
REACT_APP_OUTLOOK_CLIENT_ID=your_outlook_client_id
REACT_APP_OUTLOOK_CLIENT_SECRET=your_outlook_client_secret

# Zoom Integration
REACT_APP_ZOOM_CLIENT_ID=your_zoom_client_id
REACT_APP_ZOOM_CLIENT_SECRET=your_zoom_client_secret

# Teams Integration
REACT_APP_TEAMS_CLIENT_ID=your_teams_client_id
REACT_APP_TEAMS_CLIENT_SECRET=your_teams_client_secret

# WebEx Integration
REACT_APP_WEBEX_CLIENT_ID=your_webex_client_id
REACT_APP_WEBEX_CLIENT_SECRET=your_webex_client_secret

# Elasticsearch Configuration
REACT_APP_ELASTICSEARCH_URL=http://localhost:9200
REACT_APP_ELASTICSEARCH_INDEX=ai_meeting_copilot

# Feature Flags
REACT_APP_ENABLE_WEBSOCKETS=true
REACT_APP_ENABLE_CALENDAR_SYNC=true
REACT_APP_ENABLE_EXTERNAL_PLATFORMS=true
REACT_APP_ENABLE_ADVANCED_SEARCH=true
REACT_APP_ENABLE_SSO=true
REACT_APP_ENABLE_ENTERPRISE_FEATURES=true

# Analytics
REACT_APP_ANALYTICS_ENABLED=true
REACT_APP_ANALYTICS_ENDPOINT=http://localhost:5000/api/analytics

# Security
REACT_APP_ENABLE_2FA=true
REACT_APP_SESSION_TIMEOUT=3600000

# Development
REACT_APP_DEBUG_MODE=false
REACT_APP_LOG_LEVEL=info
